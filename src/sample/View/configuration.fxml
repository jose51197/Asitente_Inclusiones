<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.Configuration">
   <children>
      <Label text="Configuracion del asistente">
         <font>
            <Font size="16.0" />
         </font></Label>
      <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0">
         <children>
            <Label prefHeight="25.0" text="Periodo de inclusiones:" />
            <Separator prefWidth="5.0" />
            <TextField id="tbox_semestre" fx:id="tbox_periodo" promptText="Semestre" />
         </children>
      </HBox>
      <Label text="Carga de datos">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
       <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0">
           <children>
               <Label prefHeight="25.0" text="Datos de estudiantes:" />
               <Separator prefWidth="5.0" />
            <Button fx:id="btn_cargarEstudiantes" mnemonicParsing="false" text="Cargar" />
           </children>
       </HBox>

       <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0">
           <children>
               <Label prefHeight="25.0" text="Plan de estudios:" />
               <Separator prefWidth="5.0" />
               <Button fx:id="btn_cargarPlan" mnemonicParsing="false" text="Cargar" />
           </children>
       </HBox>

       <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0">
           <children>
               <Label prefHeight="25.0" text="Oferta del semestre" />
               <Separator prefWidth="5.0" />
               <Button fx:id="btn_cargarOfertaSem" mnemonicParsing="false" text="Cargar" />
           </children>
       </HBox>

       <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0">
           <children>
               <Label prefHeight="25.0" text="Inclusiones registradas" />
               <Separator prefWidth="5.0" />
               <Button fx:id="btn_cargarInclusiones" mnemonicParsing="false" text="Cargar" />
           </children>
       </HBox>

   </children>
   <padding>
      <Insets left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
