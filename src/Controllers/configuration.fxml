                      </children>
                  </HBox>

                  <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0">
                      <children>
                          <Label prefHeight="25.0" text="InformaciÃ³n de estudiantes" />
                          <Separator prefWidth="5.0" />
                          <Button fx:id="btn_cargarEstudiantes" mnemonicParsing="false" onAction="#cargarInfoEstudiantes" text="Seleccionar Archivo" />
                     <Label fx:id="nameInfoEstudiantes" prefHeight="17.0" prefWidth="193.0" />
                      </children>
                  </HBox>

                  <HBox alignment="CENTER" prefHeight="45.0" prefWidth="200.0">
                      <children>
                          <Separator prefWidth="5.0" />
                          <Button fx:id="btn_cargarDatosBase" mnemonicParsing="false" onAction="#cargarDatosBase" text="Cargar datos" />
                      </children>
                  </HBox>
              </VBox>


            </content>
          </Tab>
          <Tab text="Inclusiones">
            <content>


                <VBox minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <Label text="Carga de datos">
                        <font>
                            <Font size="14.0" />
                        </font>
                    </Label>
                    <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0">
                        <children>
                            <Label prefHeight="25.0" text="Cargar inclusiones:" />
                            <Separator prefWidth="5.0" />
                            <Button fx:id="btn_cargarInclusiones" mnemonicParsing="false" onAction="#cargarInclusiones" text="Seleccionar Archivo" />
                     <Label fx:id="nameInclusiones" prefHeight="17.0" prefWidth="239.0" />
                        </children>
                    </HBox>
                    <HBox alignment="CENTER" prefHeight="45.0" prefWidth="200.0">
                        <children>
                            <Separator prefWidth="5.0" />
                            <Button fx:id="btn_cargarDatosInclusiones" mnemonicParsing="false" onAction="#cargarDatosInclusiones" text="Cargar datos" />
